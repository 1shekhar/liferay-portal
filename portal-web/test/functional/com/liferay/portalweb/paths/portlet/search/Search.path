<html>
<head>
<title>Search</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">Search</td></tr>
</thead>
<tbody>
<tr>
	<td>CLASSIC_SEARCH_BAR</td>
	<td>//input[contains(@id,'templateSearch') and @type='text']</td>
	<td></td>
</tr>
<tr>
	<td>FACET_CONFIGURE</td>
	<td>//div[label[contains(.,'${key_facetName}')]]/following-sibling::div//a[contains(.,'Configure')]</td>
	<td></td>
</tr>
<tr>
	<td>INCREMENTAL_SEARCH_SUGGESTION</td>
	<td>//div[@class='yui3-aclist-content']//li[contains(@data-text,"${key_kbArticleTitle}")]</td>
	<td></td>
</tr>
<tr>
	<td>SEARCH_BAR</td>
	<td>//input[not(ancestor::header[@id='banner'])][@title='Search']</td>
	<td></td>
</tr>
<tr>
	<td>SEARCH_BAR_ELLIPSIS_ICON</td>
	<td>//header[@class='portlet-topper' and contains(.,'Search Bar')][not(ancestor::header[@id='banner'])]//div[contains(@class,'portlet-options')]/a</td>
	<td></td>
</tr>
<tr>
	<td>SEARCH_BAR_SCOPE_SELECT</td>
	<td>//div[not(ancestor::header[@id='banner'])]/select[@title='Scope']</td>
	<td></td>
</tr>
<tr>
	<td>SEARCH_INFO</td>
	<td>//span[contains(@class,'text-truncate')]/span</td>
	<td></td>
</tr>
<tr>
	<td>SEARCH_RESULTS</td>
	<td>//div[@class='search-results']</td>
	<td></td>
</tr>
<tr>
	<td>SEARCH_PORTLET_OPTIONS_WRENCH</td>
	<td>//div[contains(@class,'portlet-options')]/a[contains(@id,'_SearchPortlet') and @title='Options']</td>
	<td></td>
</tr>
<tr>
	<td>SETUP_DESTINATION</td>
	<td>//a[.='Set up its destination to make it visible.']</td>
	<td></td>
</tr>
<tr>
	<td>TEMPLATE_SEARCH_BAR</td>
	<td>//header[@id='banner']//input[@title='Search']</td>
	<td></td>
</tr>
<tr>
	<td>TEMPLATE_SEARCH_BAR_ELLIPSIS_ICON</td>
	<td>//header[@class='portlet-topper' and contains(.,'Search')][ancestor::header[@id='banner']]//div[contains(@class,'portlet-options')]/a</td>
	<td></td>
</tr>
<tr>
	<td>TEMPLATE_SEARCH_BAR_SCOPE_SELECT</td>
	<td>//header[@id='banner']//div/select[@title='Scope']</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>