<definition>
	<var name="assetType" value="Document" />
	<var name="dmDocumentTypeName" value="Basic Document" />

	<command name="addCategories">
		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Categorization" />
		</execute>

		<execute macro="AssetCategorization#addMultipleCategories">
			<var name="categoryNameList" value="${categoryNameList}" />
			<var name="vocabularyName" value="${vocabularyName}" />
		</execute>
	</command>

	<command name="addContractCP">
		<execute macro="DMNavigator#gotoAddDocumentType">
			<var name="dmDocumentTypeName" value="Contract" />
		</execute>

		<if>
			<isset var="dmContractFile" />
			<then>
				<execute function="UploadCommonFile" locator1="TextInput#FILE" value1="${dmContractFile}" />
			</then>
		</if>

		<execute macro="PortletEntry#inputTitle">
			<var name="title" value="${dmContractTitle}" />
		</execute>

		<if>
			<isset var="dmContractDescription" />
			<then>
				<execute macro="PortletEntry#inputDescription">
					<var name="description" value="${dmContractDescription}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="effectiveDate" />
			<then>
				<execute function="Type#clickAtType" locator1="TextInput#GENERIC_TEXT_INPUT" value1="${effectiveDate}">
					<var name="key_fieldLabel" value="Effective Date" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="expirationDate" />
			<then>
				<execute function="Type#clickAtType" locator1="TextInput#GENERIC_TEXT_INPUT" value1="${expirationDate}">
					<var name="key_fieldLabel" value="Expiration Date" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="contractType" />
			<then>
				<execute function="Select" locator1="Select#GENERIC_SELECT_FIELD" value1="${contractType}">
					<var name="key_fieldLabel" value="Contract Type" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="status" />
			<then>
				<execute function="Select" locator1="Select#GENERIC_SELECT_FIELD" value1="${status}">
					<var name="key_fieldLabel" value="Status" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="dmContractLegalReviewer" />
			<then>
				<execute function="Type" locator1="TextInput#GENERIC_TEXT_INPUT" value1="${dmContractLegalReviewer}">
					<var name="key_fieldLabel" value="Legal Reviewer" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="dmContractSigningAuthority" />
			<then>
				<execute function="Type" locator1="TextInput#GENERIC_TEXT_INPUT" value1="${dmContractSigningAuthority}">
					<var name="key_fieldLabel" value="Signing Authority" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="dmContractDealName" />
			<then>
				<execute function="Type" locator1="TextInput#GENERIC_TEXT_INPUT" value1="${dmContractDealName}">
					<var name="key_fieldLabel" value="Deal Name" />
				</execute>
			</then>
		</if>

		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="addCP">
		<execute macro="DMNavigator#gotoAddDocumentType">
			<var name="dmDocumentTypeName" value="${dmDocumentTypeName}" />
		</execute>

		<if>
			<equals arg1="${uploadTempFile}" arg2="true" />
			<then>
				<execute function="UploadTempFile" locator1="TextInput#FILE" value1="${dmDocumentFile}" />
			</then>
			<elseif>
				<equals arg1="${nullFile}" arg2="true" />
				<then>
					<execute function="AssertElementPresent" locator1="TextInput#FILE" />
				</then>
			</elseif>
			<else>
				<if>
					<isset var="dmDocumentFile" />
					<then>
						<execute function="UploadCommonFile" locator1="TextInput#FILE" value1="${dmDocumentFile}" />
					</then>
				</if>
			</else>
		</if>

		<if>
			<isset var="tagNameList" />
			<then>
				<execute macro="Panel#expandPanel">
					<var name="panelHeading" value="Categorization" />
				</execute>

				<for list="${tagNameList}" param="tagName">
					<execute function="Click#clickAt" locator1="AssetCategorization#TAGS_ADD_BUTTON" />

					<execute function="Type" locator1="AssetCategorization#TAGS_FIELD" value1="${tagName}" />
				</for>

				<execute function="Click#clickAt" locator1="AssetCategorization#TAGS_ADD_BUTTON" />
			</then>
		</if>

		<if>
			<isset var="vocabularyName" />
			<then>
				<execute macro="DMDocument#addCategories">
					<var name="categoryNameList" value="${categoryNameList}" />
					<var name="vocabularyName" value="${vocabularyName}" />
				</execute>
			</then>
		</if>

		<execute macro="PortletEntry#inputTitle">
			<var name="title" value="${dmDocumentTitle}" />
		</execute>

		<if>
			<isset var="dmDocumentDescription" />
			<then>
				<execute macro="PortletEntry#inputDescription">
					<var name="description" value="${dmDocumentDescription}" />
				</execute>
			</then>
		</if>

		<execute macro="PortletEntry#publish" />
	</command>

	<command name="addDMMarketingBannerCP">
		<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />

		<execute macro="LexiconEntry#gotoAddMenuItem">
			<var name="menuItem" value="Marketing Banner" />
		</execute>

		<execute function="UploadCommonFile" locator1="TextInput#FILE" value1="${dmDocumentFile}" />

		<execute macro="PortletEntry#inputTitle">
			<var name="title" value="${dmDocumentTitle}" />
		</execute>

		<execute macro="PortletEntry#inputDescription">
			<var name="description" value="${dmDocumentDescription}" />
		</execute>

		<execute function="Select" locator1="DocumentsAndMediaEditMarketingBanner#MARKETING_SELECT" value1="${marketingSelect}" />
		<execute function="Select" locator1="DocumentsAndMediaEditMarketingBanner#CAMPAIGN_THEME_SELECT" value1="${campaignTheme}" />
		<execute function="Select" locator1="DocumentsAndMediaEditMarketingBanner#BUSINESS_GOAL_SELECT" value1="${businessGoal}" />
		<execute function="Click" locator1="DocumentsAndMediaEditMarketingBanner#NEEDS_LEGAL_REVIEW_${legalReview}_RADIO" />
		<execute function="Type" locator1="DocumentsAndMediaEditMarketingBanner#BANNER_NAME_FIELD" value1="${bannerName}" />
		<execute function="Type" locator1="DocumentsAndMediaEditMarketingBanner#BANNER_DESCRIPTION_FIELD" value1="${bannerDescription}" />

		<execute macro="PortletEntry#publish" />
	</command>

	<command name="addDMShortcutCP">
		<execute macro="DMNavigator#gotoAddDocumentType">
			<var name="dmDocumentTypeName" value="Shortcut" />
		</execute>

		<execute function="Click" locator1="DocumentsAndMediaAddShortcut#SELECT_SITE_BUTTON" />

		<execute macro="IFrame#selectModalFrame" />

		<execute function="AssertClick" locator1="SitesSelectSite#CHOOSE_BUTTON" value1="Choose">
			<var name="key_siteName" value="${siteName}" />
		</execute>

		<execute function="SelectFrameTop" value1="relative=top" />

		<execute function="Click" locator1="DocumentsAndMediaAddShortcut#SELECT_DOCUMENT_BUTTON" />

		<execute macro="IFrame#selectModalFrame" />

		<if>
			<isset var="dmFolderName" />
			<then>
				<execute function="AssertClick" locator1="DocumentsAndMediaAddShortcut#SELECT_FILE_FOLDER_NAME" value1="${dmFolderName}">
					<var name="key_dmFolderName" value="${dmFolderName}" />
				</execute>
			</then>
		</if>

		<execute function="AssertClick#assertPartialTextClickAt" locator1="DocumentsAndMediaAddShortcut#SELECT_FILE_DOCUMENT_TITLE" value1="${dmDocumentTitle}">
			<var name="key_dmDocumentTitle" value="${dmDocumentTitle}" />
		</execute>

		<execute function="SelectFrameTop" value1="relative=top" />

		<execute macro="PortletEntry#save" />
	</command>

	<command name="addDocumentTypeCmdCP">
		<execute macro="DMNavigator#gotoAddDocumentType">
			<var name="dmDocumentTypeName" value="${dmDocumentTypeName}" />
		</execute>

		<execute function="UploadCommonFile" locator1="TextInput#FILE" value1="${dmDocumentFile}" />

		<execute macro="PortletEntry#inputTitle">
			<var name="title" value="${dmDocumentTitle}" />
		</execute>

		<execute macro="PortletEntry#inputDescription">
			<var name="description" value="${dmDocumentDescription}" />
		</execute>

		<if>
			<and>
				<isset var="date" />
				<isset var="dateFieldName" />
			</and>
			<then>
				<execute function="Type" locator1="DocumentsAndMediaEditCustomDocumentType#DATE_FIELD" value1="${date}">
					<var name="key_fieldName" value="${dateFieldName}" />
				</execute>
			</then>
			<elseif>
				<isset var="date" />
				<then>
					<execute function="Type" locator1="DocumentsAndMediaEditCustomDocumentType#DATE_FIELD" value1="${date}">
						<var name="key_fieldName" value="date" />
					</execute>
				</then>
			</elseif>
		</if>

		<if>
			<isset var="selectOption" />
			<then>
				<execute function="Select" locator1="DocumentsAndMediaEditCustomDocumentType#SELECT_OPTIONS_SELECT" value1="${selectOption}" />
			</then>
		</if>

		<if>
			<and>
				<isset var="textBox" />
				<isset var="textBoxFieldName" />
			</and>
			<then>
				<execute function="Type" locator1="DocumentsAndMediaEditCustomDocumentType#TEXT_BOX_FIELD" value1="${textBox}">
					<var name="key_fieldName" value="${textBoxFieldName}" />
				</execute>
			</then>
			<elseif>
				<isset var="textBox" />
				<then>
					<execute function="Type" locator1="DocumentsAndMediaEditCustomDocumentType#TEXT_BOX_FIELD" value1="${textBox}">
						<var name="key_fieldName" value="textBox" />
					</execute>
				</then>
			</elseif>
		</if>

		<if>
			<isset var="textField" />
			<then>
				<execute function="Type" locator1="DocumentsAndMediaEditCustomDocumentType#TEXT_FIELD" value1="${textField}">
					<var name="key_fieldName" value="Text" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="numberField" />
			<then>
				<execute function="Type" locator1="DocumentsAndMediaEditCustomDocumentType#NUMBER_FIELD" value1="${numberField}">
					<var name="key_fieldName" value="Number" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="booleanField" />
			<then>
				<execute function="Check" locator1="DocumentsAndMediaEditCustomDocumentType#BOOLEAN_CHECKBOX" value1="${booleanField}">
					<var name="key_fieldName" value="Boolean" />
				</execute>
			</then>
		</if>
	</command>

	<command name="addDocumentTypeCP">
		<execute macro="DMDocument#addDocumentTypeCmdCP">
			<var name="booleanField" value="${booleanField}" />
			<var name="date" value="${date}" />
			<var name="dateFieldName" value="${dateFieldName}" />
			<var name="dmDocumentDescription" value="${dmDocumentDescription}" />
			<var name="dmDocumentFile" value="${dmDocumentFile}" />
			<var name="dmDocumentTitle" value="${dmDocumentTitle}" />
			<var name="dmDocumentTypeName" value="${dmDocumentTypeName}" />
			<var name="numberField" value="${numberField}" />
			<var name="selectOption" value="${selectOption}" />
			<var name="textBox" value="${textBox}" />
			<var name="textBoxFieldName" value="${textBoxFieldName}" />
			<var name="textField" value="${textField}" />
		</execute>

		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="addDocumentTypePG">
		<execute macro="Portlet#clickPortletPlusIconPG">
			<var name="portletName" value="Documents and Media" />
		</execute>

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="${dmDocumentTypeName}">
			<var name="key_menuItem" value="${dmDocumentTypeName}" />
		</execute>

		<execute macro="DMDocument#editDocument">
			<var name="dmDocumentDescription" value="${dmDocumentDescription}" />
			<var name="dmDocumentFile" value="${dmDocumentFile}" />
			<var name="dmDocumentTitle" value="${dmDocumentTitle}" />
		</execute>

		<if>
			<isset var="selectOption" />
			<then>
				<execute function="Select" locator1="DocumentsAndMediaEditCustomDocumentType#SELECT_OPTIONS_SELECT" value1="${selectOption}" />
			</then>
		</if>

		<if>
			<isset var="textField" />
			<then>
				<execute function="Type" locator1="DocumentsAndMediaEditCustomDocumentType#TEXT_FIELD" value1="${textField}">
					<var name="key_fieldName" value="Text" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="numberField" />
			<then>
				<execute function="Type" locator1="DocumentsAndMediaEditCustomDocumentType#NUMBER_FIELD" value1="${numberField}">
					<var name="key_fieldName" value="Number" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="booleanField" />
			<then>
				<execute function="Check" locator1="DocumentsAndMediaEditCustomDocumentType#BOOLEAN_CHECKBOX" value1="${booleanField}">
					<var name="key_fieldName" value="Boolean" />
				</execute>
			</then>
		</if>

		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="addDocumentTypeWithWorkflowCP">
		<execute macro="DMDocument#addDocumentTypeCmdCP">
			<var name="booleanField" value="${booleanField}" />
			<var name="dmDocumentDescription" value="${dmDocumentDescription}" />
			<var name="dmDocumentFile" value="${dmDocumentFile}" />
			<var name="dmDocumentTitle" value="${dmDocumentTitle}" />
			<var name="dmDocumentTypeName" value="${dmDocumentTypeName}" />
			<var name="numberField" value="${numberField}" />
			<var name="selectOption" value="${selectOption}" />
			<var name="textField" value="${textField}" />
		</execute>

		<execute function="AssertClick" locator1="Button#SUBMIT_FOR_PUBLICATION" value1="Submit for Publication" />

		<execute macro="Alert#viewSuccessMessage" />
	</command>

	<command name="addMultipleCP">
		<execute macro="LexiconEntry#gotoAdd" />

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="Multiple Documents">
			<var name="key_menuItem" value="Multiple Documents" />
		</execute>

		<if>
			<isset var="dmDocumentFileList" />
			<then>
				<for list="${dmDocumentFileList}" param="dmDocumentFile">
					<execute function="AssertClick" locator1="DynamicUploader#SELECT_FILES_BUTTON" value1="Select Files" />

					<if>
						<equals arg1="${uploadTempFile}" arg2="true" />
						<then>
							<execute function="SikuliUploadTempFile" locator1="OS#FILE_DIALOG_BOX_FILE_NAME_FIELD_PNG" value1="${dmDocumentFile}" />
						</then>
						<else>
							<execute function="SikuliUploadCommonFile" locator1="OS#FILE_DIALOG_BOX_FILE_NAME_FIELD_PNG" value1="${dmDocumentFile}" />
						</else>
					</if>

					<execute function="AssertChecked" locator1="DynamicUploader#SELECT_ALL_CHECKBOX" />

					<execute function="AssertElementPresent" locator1="DynamicUploader#ALL_FILES_READY_TO_BE_SAVED_DOCUMENT_TITLE">
						<var name="key_documentName" value="${dmDocumentFile}" />
					</execute>
				</for>
			</then>
		</if>

		<if>
			<isset var="viewableBy" />
			<then>
				<execute function="Select" locator1="DocumentsAndMediaEditFolder#PERMISSIONS_SELECT" value1="${viewableBy}" />
			</then>
		</if>

		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute function="AssertTextEquals" locator1="Message#SUCCESS" value1="Your request completed successfully." />
	</command>

	<command name="addMultiplePG">
		<execute macro="Portlet#clickPortletPlusIconPG">
			<var name="portletName" value="Documents and Media" />
		</execute>

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="Multiple Documents">
			<var name="key_menuItem" value="Multiple Documents" />
		</execute>

		<if>
			<isset var="dmDocumentFileList" />
			<then>
				<for list="${dmDocumentFileList}" param="dmDocumentFile">
					<execute function="AssertClick" locator1="DynamicUploader#SELECT_FILES_BUTTON" value1="Select Files" />

					<if>
						<equals arg1="${uploadTempFile}" arg2="true" />
						<then>
							<execute function="SikuliUploadTempFile" locator1="OS#FILE_DIALOG_BOX_FILE_NAME_FIELD_PNG" value1="${dmDocumentFile}" />
						</then>
						<else>
							<execute function="SikuliUploadCommonFile" locator1="OS#FILE_DIALOG_BOX_FILE_NAME_FIELD_PNG" value1="${dmDocumentFile}" />
						</else>
					</if>

					<execute function="AssertChecked" locator1="DynamicUploader#SELECT_ALL_CHECKBOX" />

					<execute function="AssertElementPresent" locator1="DynamicUploader#ALL_FILES_READY_TO_BE_SAVED_DOCUMENT_TITLE">
						<var name="key_documentName" value="${dmDocumentFile}" />
					</execute>
				</for>
			</then>
		</if>

		<if>
			<isset var="viewableBy" />
			<then>
				<execute function="Select" locator1="DocumentsAndMediaEditFolder#PERMISSIONS_SELECT" value1="${viewableBy}" />
			</then>
		</if>

		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute function="AssertTextEquals" locator1="Message#SUCCESS" value1="Your request completed successfully." />
	</command>

	<command name="addPG">
		<if>
			<equals arg1="${stagingActivated}" arg2="true" />
			<then>
				<execute function="AssertElementNotPresent" locator1="Message#WORKFLOW_UNAPPROVED" />
			</then>
		</if>

		<execute macro="Portlet#clickPortletPlusIconPG">
			<var name="portletName" value="Documents and Media" />
		</execute>

		<if>
			<isset var="dmDocumentTypeName" />
			<then>
				<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="${dmDocumentTypeName}">
					<var name="key_menuItem" value="${dmDocumentTypeName}" />
				</execute>
			</then>
			<else>
				<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="Basic Document">
					<var name="key_menuItem" value="Basic Document" />
				</execute>
			</else>
		</if>

		<execute macro="DMDocument#editDocument">
			<var name="dmDocumentDescription" value="${dmDocumentDescription}" />
			<var name="dmDocumentFile" value="${dmDocumentFile}" />
			<var name="dmDocumentTitle" value="${dmDocumentTitle}" />
		</execute>

		<if>
			<isset var="categoryName" />
			<then>
				<execute macro="Panel#expandPanel">
					<var name="panelHeading" value="Categorization" />
				</execute>

				<execute function="AssertClick" locator1="Button#SELECT" value1="Select" />

				<var name="key_categoryName" value="${categoryName}" />

				<execute function="AssertClick" locator1="AssetCategorization#CATEGORIES_CATEGORY" value1="${categoryName}" />

				<execute function="Click" locator1="Icon#CLOSE" />

				<execute function="AssertTextEquals" locator1="AssetCategorization#CATEGORY" value1="${categoryName}" />
			</then>
		</if>

		<if>
			<isset var="dmDocumentRelatedAsset" />
			<then>
				<execute macro="DMDocument#addRelatedAssetPG">
					<var name="assetTitle" value="${assetTitle}" />
					<var name="assetType" value="${assetType}" />
				</execute>
			</then>
		</if>

		<if>
			<isset var="tagNameList" />
			<then>
				<for list="${tagNameList}" param="tagName">
					<execute macro="Panel#expandPanel">
						<var name="panelHeading" value="Categorization" />
					</execute>

					<execute function="Type" locator1="AssetCategorization#TAGS_FIELD" value1="${tagName}" />

					<execute function="AssertClick" locator1="AssetCategorization#TAGS_ADD_BUTTON" value1="Add" />
				</for>
			</then>
		</if>

		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<if>
			<equals arg1="${dmDocument}" arg2="Duplicate" />
			<then>
				<execute macro="Alert#viewRequestFailedToComplete" />

				<execute function="AssertTextEquals#assertPartialText" locator1="DocumentsAndMediaEditDocument#UNIQUE_DOCUMENT_ERROR_MESSAGE" value1="Please enter a unique document name." />
			</then>
			<elseif>
				<equals arg1="${dmDocument}" arg2="Invalid" />
				<then>
					<execute macro="Alert#viewRequestFailedToComplete" />

					<execute function="AssertTextEquals#assertPartialText" locator1="Message#ERROR_2" value1="Please enter a file with a valid file name." />
				</then>
			</elseif>
			<else>
				<execute macro="Alert#viewSuccessMessage" />
			</else>
		</if>
	</command>

	<command name="addPGViaAP">
		<execute macro="AssetPublisherPortlet#addAsset">
			<var name="assetType" value="Basic Document" />
		</execute>

		<execute macro="DMDocument#editDocument">
			<var name="dmDocumentDescription" value="${dmDocumentDescription}" />
			<var name="dmDocumentFile" value="${dmDocumentFile}" />
			<var name="dmDocumentTitle" value="${dmDocumentTitle}" />
		</execute>

		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute function="SelectFrame" locator1="relative=top" />
	</command>

	<command name="addPGViaAPWithValidationPosition">
		<execute macro="AssetPublisherPortlet#addAsset">
			<var name="assetType" value="Basic Document" />
		</execute>

		<execute macro="DMDocument#editDocument">
			<var name="dmDocumentDescription" value="${dmDocumentDescription}" />
			<var name="dmDocumentFile" value="${dmDocumentFile}" />
			<var name="dmDocumentTitle" value="${dmDocumentTitle}" />
		</execute>

		<execute function="AssertElementPresent" locator1="Button#CANCEL_GIVEN_POSITION" />
		<execute function="AssertElementPresent" locator1="Button#PUBLISH_GIVEN_POSITION" />

		<execute function="AssertClick" locator1="Button#PUBLISH" value1="Publish" />

		<execute function="SelectFrame" locator1="relative=top" />
	</command>

	<command name="addPGViaMG">
		<execute macro="Portlet#clickPortletPlusIconPG">
			<var name="portletName" value="Media Gallery" />
		</execute>

		<execute function="AssertClick" locator1="MenuItem#ANY_MENU_ITEM" value1="Basic Document">
			<var name="key_menuItem" value="Basic Document" />
		</execute>

		<execute function="UploadCommonFile" locator1="TextInput#FILE" value1="${mgDocumentFile}" />

		<execute macro="PortletEntry#inputTitle">
			<var name="title" value="${mgDocumentTitle}" />
		</execute>

		<execute macro="PortletEntry#inputDescription">
			<var name="description" value="${mgDocumentDescription}" />
		</execute>

		<execute macro="PortletEntry#publish" />
	</command>

	<command name="addRelatedAssetPG">
		<execute macro="Panel#expandPanel">
			<var name="panelHeading" value="Related Assets" />
		</execute>

		<execute function="AssertClick" locator1="AssetRelatedAssets#RELATED_ASSETS_SELECT" value1="Select" />

		<execute function="AssertClick" locator1="AssetRelatedAssets#RELATED_ASSETS_SELECT_MENULIST_ASSET" value1="${assetType}">
			<var name="key_assetType" value="${assetType}" />
		</execute>

		<execute macro="IFrame#selectDialogFrame" />

		<execute function="AssertClick#assertPartialTextClickAt" locator1="AssetRelatedAssets#ASSET_TABLE_CHOOSE" value1="${assetType}">
			<var name="key_assetTitle" value="${assetTitle}" />
